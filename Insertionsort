#include<stdio.h>
void main()
{
    int i,n, arr[10];
    printf("Enter the size of the array;");
    scanf("%d",&n);
    printf("Enter the array---");
    for(i=0;i<n;i++)
    {
        printf("\n Enter %d element= ", i+1);
        scanf("%d", & arr[i]);
        
    }
    //code for insection sort
    printf("\n sorted array is----");
    for(i=0;i<n;i++)
    printf("\t %d",arr[i]);
}

/*
output:
Enter the size of the array;5
Enter the array---
 Enter 1 element= 1
 Enter 2 element= 2
 Enter 3 element= 3
 Enter 4 element= 4
 Enter 5 element= 5
 sorted array is----	 1	 2	 3	 4	 5
 */
