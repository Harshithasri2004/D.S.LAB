#include<stdio.h>
int main()
{
    int arr[20],size,i,j,temp;
    printf("Enter the size of array");
    scanf("%d", &size);
    printf("Enter the elements of the array,,,,,");
    for(i=0;i<size;i++)
    {
        printf("\n Array in %d element",i+1);
        scanf("%d", &arr[i]);
    }
    if(i=0,i<size)
    printf("/n sorted array is,,,,,,");
    for(i=0;i<size;i++)
    printf("\t%d",arr[i]);
    return 0;
}

*\
output
Enter the size of array4
Enter the elements of the array,,,,,
 Array in 1 element1
 Array in 2 element2
 Array in 3 element3
 Array in 4 element4
 /n sorted array is,,,,,,1234
 
